<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>test.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UserHomeServlet</servlet-name>
		<servlet-class>test.UserHomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserHomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetProductsServlet</servlet-name>
		<servlet-class>test.GetProductsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetProductsServlet</servlet-name>
		<url-pattern>/getProducts</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddProductsServlet</servlet-name>
		<servlet-class>test.AddProductsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddProductsServlet</servlet-name>
		<url-pattern>/addProducts</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetCartDetailsServlet</servlet-name>
		<servlet-class>test.GetCartDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCartDetailsServlet</servlet-name>
		<url-pattern>/getCart</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>test.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Login.html</welcome-file>
	</welcome-file-list>
</web-app>